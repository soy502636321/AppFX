<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="fx.hq.ui.entity.BaseOrganEntity" table="BASE_ORGAN">
    	<id name="organPid" type="java.lang.String">
    		<column name="ORGAN_PID" />
    	</id>
    	<property name="organSms" type="java.lang.String">
    		<column name="ORGAN_SMS" />
    	</property>
    	<property name="remark" type="java.lang.String">
    		<column name="REMARK" />
    	</property>
    </class>
</hibernate-mapping>
